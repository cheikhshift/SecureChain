{
  "manifest_version": 2,

  "name": "Secure String",
  "description": "Decrypt and encrypt messages with peers from your Network.",
  "version": "1.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}